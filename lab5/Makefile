all:
	gcc src/mm1.c -o bin/mm1 -O0 -lpapi -static
	gcc src/mm2.c -o bin/mm2 -O0 -lpapi -static
	gcc src/mm3.c -o bin/mm3 -O0 -lpapi -static
	gcc src/mm4.c -o bin/mm4 -O0 -lpapi -static
	gcc src/mm5.c -o bin/mm5 -O0 -lpapi -static
	gcc src/mm6.c -o bin/mm6 -O0 -lpapi -static
	gcc src/mm7.c -o bin/mm7 -O0 -lpapi -static
	gcc src/mm8.c -o bin/mm8 -O0 -lpapi -static
	gcc src/mm9.c -o bin/mm9 -O0 -lpapi -static
	gcc src/mm10.c -o bin/mm10 -O0 -lpapi -static
	
run:
	./bin/mm1 0 >> wynik0
	./bin/mm2 0 >> wynik0
	./bin/mm3 0 >> wynik0
	./bin/mm4 0 >> wynik0
	./bin/mm5 0 >> wynik0
	./bin/mm6 0 >> wynik0
