	./bin/mm4 1 >> wynik1
	./bin/mm5 1 >> wynik1
	./bin/mm6 1 >> wynik1
	./bin/mm7 1 >> wynik1
	./bin/mm8 1 >> wynik1
	./bin/mm9 1 >> wynik1
	./bin/mm10 1 >> wynik1
